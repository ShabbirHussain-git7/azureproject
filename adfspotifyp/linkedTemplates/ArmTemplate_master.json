{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfspotifyp"
		},
		"linkDatalakeDump_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkDatalakeDump'"
		},
		"linkSql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linkSql'"
		},
		"linkDatalakeDump_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagespotifyprojec1.dfs.core.windows.net/"
		},
		"linkSql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azurespotifyproject01.database.windows.net"
		},
		"linkSql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "azurespotifydbshabbir"
		},
		"linkSql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminshabbir"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfspotifyp_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linkDatalakeDump_accountKey": {
						"value": "[parameters('linkDatalakeDump_accountKey')]"
					},
					"linkSql_password": {
						"value": "[parameters('linkSql_password')]"
					},
					"linkDatalakeDump_properties_typeProperties_url": {
						"value": "[parameters('linkDatalakeDump_properties_typeProperties_url')]"
					},
					"linkSql_properties_typeProperties_server": {
						"value": "[parameters('linkSql_properties_typeProperties_server')]"
					},
					"linkSql_properties_typeProperties_database": {
						"value": "[parameters('linkSql_properties_typeProperties_database')]"
					},
					"linkSql_properties_typeProperties_userName": {
						"value": "[parameters('linkSql_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}